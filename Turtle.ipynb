    "wn = turtle.Screen()\n",
    "Tex = turtle.Turtle()\n",
    "dist = 15\n",
    "angle =30\n",
    "Tex.color(\"red\")\n",
    "Tex.shape(\"turtle\")\n",
    "Tex.penup()\n",
    "for i in range(12):\n",
    "    Tex.stamp()\n",
    "    Tex.forward(dist)\n",
    "    Tex.left(angle)\n",
    "    angle +=2\n",
    "    dist +=3\n"
